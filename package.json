{
  "name": "@octokit/plugin-enterprise-compatibility",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "description": "Octokit plugin for improving GHE compatibility",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "tap --coverage-report=html",
    "generate-routes": "node scripts/generate-routes",
    "lint": "prettier --check index.js 'test/*' README.md package.json",
    "lint:fix": "prettier --write index.js 'test/*' README.md package.json",
    "pretest": "npm run -s lint",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octokit/plugin-enterprise-compatibility.js.git"
  },
  "keywords": [
    "octokit",
    "github",
    "api",
    "rest",
    "plugin",
    "enterprise"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octokit/plugin-enterprise-compatibility.js/issues"
  },
  "homepage": "https://github.com/octokit/plugin-enterprise-compatibility.js#readme",
  "devDependencies": {
    "@octokit/rest": "^16.24.2",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "fetch-mock": "^7.5.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.12",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {},
  "jest": {
    "preset": "ts-jest",
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
