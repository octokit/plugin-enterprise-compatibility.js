{
  "name": "@octokit/plugin-enterprise-compatibility",
  "version": "0.0.0-development",
  "publishConfig": {
    "access": "public"
  },
  "description": "Octokit plugin for improving GHE compatibility",
  "scripts": {
    "build": "pika build",
    "generate-routes": "node scripts/generate-routes",
    "lint": "prettier --check index.js 'test/*' README.md package.json",
    "lint:fix": "prettier --write index.js 'test/*' README.md package.json",
    "pretest": "npm run -s lint",
    "test": "jest --coverage"
  },
  "repository": "https://github.com/octokit/plugin-enterprise-compatibility.js.git",
  "keywords": [
    "octokit",
    "github",
    "api",
    "rest",
    "plugin",
    "enterprise"
  ],
  "author": "Gregor Martynus (https://github.com/gr2m)",
  "license": "MIT",
  "dependencies": {
    "@octokit/request-error": "^1.2.0",
    "@octokit/types": "^2.0.1"
  },
  "devDependencies": {
    "@octokit/core": "^2.1.0",
    "@octokit/rest": "^16.24.2",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-build-web": "^0.8.1",
    "@pika/plugin-ts-standard-pkg": "^0.8.1",
    "@types/fetch-mock": "^7.3.1",
    "@types/jest": "^24.0.21",
    "@types/node": "^13.1.0",
    "fetch-mock": "^8.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "semantic-release": "^16.0.0",
    "semantic-release-plugin-update-version-in-files": "^1.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "preset": "ts-jest",
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./pkg"
        }
      ],
      [
        "semantic-release-plugin-update-version-in-files",
        {
          "files": [
            "pkg/dist-web/*",
            "pkg/dist-node/*",
            "pkg/*/version.*"
          ]
        }
      ]
    ]
  }
}
